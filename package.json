{
  "name": "@paxton/utils",
  "type": "module",
  "version": "0.7.2",
  "private": true,
  "packageManager": "pnpm@7.13.4",
  "description": "",
  "author": "Cheng-DX",
  "license": "MIT",
  "homepage": "https://github.com/Cheng-DX/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cheng-DX/utils.git"
  },
  "bugs": "https://github.com/Cheng-DX/utils/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "turbo-build": "turbo run build",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "play": "cd playground && nr dev",
    "presets": "tsx ./scripts/generatePresets.ts",
    "add-funs": "tsx ./scripts/addFunctions.ts",
    "patch": "nr presets && nr add-funs",
    "publish": "pnpm -r --filter './packages/**' publish"
  },
  "dependencies": {
    "@chengdx/composables": "workspace:*",
    "@chengdx/default-export-resolver": "workspace:*",
    "@chengdx/maybe-ref": "workspace:*",
    "@chengdx/naive-ui": "workspace:*",
    "@chengdx/shared": "workspace:*",
    "@chengdx/use-echarts": "workspace:*",
    "@chengdx/defa": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@vitest/ui": "^0.24.3",
    "consola": "^2.15.3",
    "eslint": "^8.26.0",
    "pkg-exports": "^0.1.2",
    "pnpm": "^7.14.0",
    "turbo": "^1.6.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vitest": "^0.23.4"
  }
}